package ChessPrj;

public class Rook extends ChessPiece{
  public String type(){
    return "Rook";
  }

  public boolean isVaildMove(Move move, IChessPiece board[][]){
    //Checks to see if the rook is moving over empty squares.
    if(moveIsOnlyOverEmptySquares(move(fromRow, fromColumn, toRow, toColumn), board[move.toRow][move.toColumn]) == false)
      return false;
    else
      return true;
  }
}
