package ChessPrj;

public class King extends ChessPiece{
  
  /* A flag variable for whether a piece has moved yet.*/
  private boolean flag; 

  public King(Player player){
    super(player);
    flag = false;
  }
  
  public String type(){
    return "King";
  }
  
  public boolean isValidMove(){
    return false;
  }

}
