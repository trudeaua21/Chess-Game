package ChessPrj;

public class Queen extends ChessPiece{
  
  /* A flag variable for whether a piece has moved yet.*/
  private boolean flag; 

  public Queen(Player player){
    super(player);
    flag = false;
  }
  
  public String type(){
    return "Queen;
  }
  
  public boolean isValidMove(){
    return false;
  }

}
